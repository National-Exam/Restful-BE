// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique  
  verified Boolean @default(false)
  password String?
}

model Employee {
   id       Int     @id @default(autoincrement())
  firstName    String 
  lastName    String 
  nationalId    String 
  telephone    String 
  email    String 
  department    String 
  position    String 
  laptopManufacturer    String 
  laptopModel    String 
 serialNumber  String
}
